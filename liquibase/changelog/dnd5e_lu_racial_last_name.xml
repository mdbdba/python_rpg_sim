    <changeSet author="mdbdba" id="lu_racial_last_name_1">
        <createTable 
                schemaName="dnd_5e"
                tableName="lu_racial_last_name">
            <column autoIncrement="true" startWith="1" name="id" type="int"/>
            <column name="race" type="varchar(32)"/>
            <column name="value" type="varchar(64)"/>
            <column name="gender" type="varchar(16)"/>
        </createTable>

        <addPrimaryKey 
            columnNames="id"
            constraintName="pk_lu_racial_last_name"
            schemaName="dnd_5e"
            tableName="lu_racial_last_name"/>
        <addDefaultValue 
            columnDataType="varchar(16)"
            columnName="gender"
            defaultValue="U"
            schemaName="dnd_5e"
            tableName="lu_racial_last_name"/>

        <addForeignKeyConstraint baseColumnNames="gender"
            baseTableName="lu_racial_last_name"
            constraintName="fk_lu_racial_last_name_gender"
            deferrable="true"
            initiallyDeferred="true"
            onDelete="CASCADE"
            onUpdate="RESTRICT"
            referencedColumnNames="value"
            referencedTableName="lu_gender"/>
    </changeSet>

