    <changeSet author="mdbdba" id="lu_racial_last_name_3">
            <createView
            replaceIfExists="true"
            schemaName="dnd_5e"
            viewName="v_alignment_preference">with tb as (SELECT race, alignment, pct_likely,
row_number() over (partition by race order by pct_likely) as orderBy
from lu_racial_alignment_preference )
, tb2 as (select race, alignment, pct_likely, orderBy, sum(pct_likely) over (partition by race order by orderby ) as upperBound,
((sum(pct_likely) over (partition by race order by orderby )) - pct_likely) as lowerBound
from tb )
, tb3 as (select race, max(orderby) as lastOrderby
from tb2
group by race )
select tb2.race, tb2.alignment, tb2.pct_likely, tb2.orderby, tb2.lowerbound, 
case when tb3.lastOrderby is not null 
     then 100
     else tb2.upperbound
     end as upperbound
from tb2
left join tb3 on tb2.race = tb3.race and tb2.orderby = tb3.lastOrderby
order by race, orderby</createView>

    </changeSet>




